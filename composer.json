{
    "name": "creode/laravel-nova-hubspot-forms",
    "description": "A Laravel Nova packages that allows you to embed a HubSpot form using a layout for Flexible Content.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "creode/laravel-hubspot-forms": "^1.0",
        "creode/nova-page-builder": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Creode\\LaravelNovaHubspotForms\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Creode\\LaravelNovaHubspotForms\\LaravelNovaHubspotFormServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
